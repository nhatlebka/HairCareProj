                }

                .service-selection li a.service-btn.active {
                    border: 1px solid #d1a95e;
                    background-color: rgba(0, 0, 0, 0.5);
                }

                .footer {
                    margin-bottom: 80px;
                }

                .half h2,
                .inner .heading-service {
                    font-family: 'Josefin Sans';
                    text-transform: uppercase;
                    font-size: 30px;
                    line-height: 1;
                    font-weight: 700;
                    color: #777777;
                }

                .inner p {
                    font-family: 'Josefin Sans';
                    font-size: 20px;
                    font-weight: 300;
                    font-style: normal;
                    color: #777777;
                    letter-spacing: 0.01em;
                    line-height: 1.8;
                }

                .inner .label-text {
                    font-family: "Josefin Sans", sans-serif;
                    text-transform: uppercase;
                    font-size: 20px;
                    font-weight: 400;
                    line-height: 2;
                    letter-spacing: 0.2em;
                    color: #777777;
                }

                .inner table td {
                    padding: 7px 30px 7px 0;
                }

                .inner .btn.btn-fixed {
                    position: relative;
                    font-family: 'Josefin Sans';
                    text-transform: uppercase;
                    border-radius: 0;
                    height: 50px;
                    width: 240px;
                    border: none;
                    outline: none;
                    background-color: #d1a95e;
                    color: #fff;
                    transition: all 0.3s;
                }

                .inner .btn.btn-fixed:hover {
                    /* border: 2px solid #d1a95e; */
                    background: #fff;
                    color: #000;
                }

                .inner .btn.btn-fixed span {
                    line-height: 40px;
                }

                .inner .btn.btn-fixed:hover .btn-before,
                .inner .btn.btn-fixed:hover .btn-after {
                    transform: scale(1);
                }

                .btn-before,
                .btn-after {
                    position: absolute;
                    top: 0;
                    left: 0;
                    height: 100%;
                    width: 100%;
                    transition: all 0.5s;
                    transform: scale(0);
                }

                .btn-before {
                    transform-origin: left bottom;
                    border-bottom: 1px solid #d1a95e;
                    border-left: 1px solid #d1a95e;
                }

                .btn-after {
                    transform-origin: right top;
                    border-right: 1px solid #d1a95e;
                    border-top: 1px solid #d1a95e;
                }

                /* On low PC */
